@{
    ViewData["Title"] = "Home Page";
}
@section Scripts
{
    <script src="~/lib/angular/angular.min.js"></script>
    <script src="~/js/PropertyController.js"></script>
    <script type='text/javascript' src='http://www.bing.com/api/maps/mapcontrol?branch=experimental&callback=loadMapScenario' async defer></script>
}
<!DOCTYPE html>
<html ng-app="app">
<head>
    <title>loadMapAsyncHTML</title>
    <meta http-equiv='Content-Type' content='text/html; charset=utf-8' />
</head>
<body ng-controller="PropertyController">
    <section id="properties">
        <div class="container">
            <div class="row">
                <h3>Dallas Furnished Homes</h3>
            </div>
        </div>
    </section>

    <section id="map">
        <div id='bingMap' style="position:relative;width:100%;height:500px;"></div>
    </section>
</body>
</html>
